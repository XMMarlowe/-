#include<stdio.h>
int main()
{
	int n,x,y,i,a[1024]={0};
	printf("请输入数组的大小\n"); 
	scanf("%d",&n);
	printf("请输入数组\n"); 
	for(i=0;i<n;i++)
	scanf("%d",&a[i]);
	printf("请输入移动的次数\n"); 
	scanf("%d",&y);
	x=y%n;
	for(i=n-x;i<n;i++)
	printf("%d ",a[i]); 
	for(i=0;i<n-x;i++)
	printf("%d ",a[i]);
	putchar('\n');
 return 0;
 }
